PREFIX was: <https://was-course.interactions.ics.unisg.ch/farm-ontology#>
PREFIX hmas: <https://purl.org/hmas/>

SELECT ?tractor WHERE {
GRAPH <https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-gero#> {
   bind (<https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-gero#farm-c79a12f6-4d1a-4705-b57f-aabf23bbdb32> as ?farm)
   ?farm a was:Farm.    
   ?farm hmas:contains ?tractor.
   ?tractor a was:Tractor.
 }
}
